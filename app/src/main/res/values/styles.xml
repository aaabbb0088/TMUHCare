<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="TextEd" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
    </style>

    <style name="TextAppearance_Header" parent="TextAppearance.MaterialCalendarWidget.Header">
        <item name="android:textSize">17sp</item>
    </style>

    <style name="TextAppearance_Date" parent="TextAppearance.MaterialCalendarWidget.Date">
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/calender_text_color</item>
    </style>

</resources>
